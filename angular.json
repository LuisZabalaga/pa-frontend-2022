{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "PA-Frontend": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "commonChunk": false,
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "moment/locale/pt-br",
              "highcharts",
              "core-js/modules/es.array.includes.js",
              "core-js/modules/es.array.slice.js",
              "core-js/modules/es.array.splice.js",
              "core-js/modules/es.array.index-of.js",
              "core-js/modules/web.dom-collections.iterator.js",
              "core-js/modules/es.string.iterator.js",
              "core-js/modules/es.object.to-string.js",
              "core-js/modules/es.map.js",
              "core-js/modules/es.array.iterator.js",
              "core-js/modules/es.object.freeze.js",
              "core-js/modules/es.symbol.description.js",
              "core-js/modules/es.symbol.js",
              "core-js/modules/es.weak-map.js",
              "core-js/modules/es.regexp.exec.js",
              "core-js/modules/es.number.constructor.js",
              "core-js/modules/es.array.fill.js",
              "core-js/modules/es.string.includes.js",
              "core-js/modules/web.dom-collections.for-each.js",
              "core-js/modules/es.string.trim.js",
              "core-js/modules/es.array.reverse.js",
              "core-js/modules/es.string.includes.js",
              "core-js/modules/es.array.index-of.js",
              "core-js/modules/es.string.trim.js",
              "core-js/modules/es.string.split.js",
              "core-js/modules/web.dom-collections.iterator.js",
              "core-js/modules/es.array.iterator.js",
              "core-js/modules/es.promise.js",
              "core-js/modules/es.regexp.to-string.js",
              "core-js/modules/es.array.reverse.js",
              "core-js/modules/es.string.ends-with.js",
              "core-js/modules/es.array.reduce.js",
              "core-js/modules/es.string.starts-with.js",
              "core-js/modules/es.string.replace.js",
              "core-js/modules/es.string.match.js",
              "core-js/modules/es.array.find-index.js",
              "core-js/modules/es.object.values.js",
              "raf",
              "core-js/modules/es.string.starts-with.js",
              "core-js/modules/web.immediate.js",
              "highcharts/modules/exporting"
            ],
            "outputPath": "dist/PA-Frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.css"
            ],
            "scripts": [
              "node_modules/@fortawesome/fontawesome-free/js/all.js",
              "node_modules/html2canvas/dist/html2canvas.min.js",
              "node_modules/jspdf/dist/jspdf.es.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "8mb",
                  "maximumError": "8mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "PA-Frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "PA-Frontend:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "PA-Frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "PA-Frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "PA-Frontend:serve:production"
            }
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy"
        }
      }
    }
  },
  "defaultProject": "PA-Frontend"
}